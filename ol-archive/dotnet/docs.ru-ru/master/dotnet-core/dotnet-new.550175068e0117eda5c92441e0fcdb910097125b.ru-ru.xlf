          <source>Examples</source>
          <target state="translated">Примеры</target>       </trans-unit>
        <trans-unit id="125" translate="yes" xml:space="preserve">
          <source>Create a C# console application project in the current directory:</source>
          <target state="translated">Создание проекта консольного приложения C# в текущем каталоге:</target>       </trans-unit>
        <trans-unit id="126" translate="yes" xml:space="preserve">
          <source><ph id="ph1">`dotnet new`</ph> or <ph id="ph2">`dotnet new --lang c#`</ph></source>
          <target state="translated"><ph id="ph1">`dotnet new`</ph> или <ph id="ph2">`dotnet new --lang c#`</ph></target>       </trans-unit>
