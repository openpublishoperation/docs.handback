        <trans-unit id="123" translate="yes" xml:space="preserve">
          <source>2.  Click the <bpt id="p1">**</bpt>Compile<ept id="p1">**</ept> tab.</source>
          <target state="translated">2.  Klicken Sie auf die <bpt id="p1">**</bpt>Kompilieren<ept id="p1">**</ept> Registerkarte.</target>       </trans-unit>
        <trans-unit id="124" translate="yes" xml:space="preserve">
          <source>3.  Click <bpt id="p1">**</bpt>Advanced<ept id="p1">**</ept>.</source>
          <target state="translated">3.  Klicken Sie auf <bpt id="p1">**</bpt>erweiterte<ept id="p1">**</ept>.</target>       </trans-unit>
