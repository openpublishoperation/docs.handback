        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>/utf8output (Visual Basic)</source>
          <target state="translated">/utf8output (Visual Basic)</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Displays compiler output using UTF-8 encoding.</source>
          <target state="translated">Visualizza l'output del compilatore usando la codifica UTF-8.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Syntax</source>
          <target state="translated">Sintassi</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Arguments</source>
          <target state="translated">Argomenti</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source><ph id="ph1">`+`</ph> &amp;#124; <ph id="ph2">`-`</ph></source>
          <target state="translated"><ph id="ph1">`+`</ph> &amp;#124; <ph id="ph2">`-`</ph></target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Optional.</source>
          <target state="translated">Facoltativo.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>The default for this option is <ph id="ph1">`/utf8output-`</ph>, which means compiler output does not use UTF-8 encoding.</source>
          <target state="translated">Il valore predefinito per questa opzione è <ph id="ph1">`/utf8output-`</ph>, pertanto non verrà utilizzata la codifica UTF-8.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Specifying <ph id="ph1">`/utf8output`</ph> is the same as specifying <ph id="ph2">`/utf8output+`</ph>.</source>
          <target state="translated">Specifica di <ph id="ph1">`/utf8output`</ph> equivale a specificare <ph id="ph2">`/utf8output+`</ph>.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>Remarks</source>
          <target state="translated">Note</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>In some international configurations, compiler output cannot be displayed correctly in the console.</source>
          <target state="translated">In alcune configurazioni internazionali, output del compilatore non può essere visualizzato correttamente nella console.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>In such situations, use <ph id="ph1">`/utf8output`</ph> and redirect compiler output to a file.</source>
          <target state="translated">In tali situazioni, utilizzare <ph id="ph1">`/utf8output`</ph> e reindirizzare l'output del compilatore a un file.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>The <ph id="ph1">`/utf8output`</ph> option is not available from within the Visual Studio development environment; it is available only when compiling from the command line.</source>
          <target state="translated">Il <ph id="ph1">`/utf8output`</ph> opzione non è disponibile all'interno dell'ambiente di sviluppo di Visual Studio, è disponibile solo durante la compilazione dalla riga di comando.</target>       </trans-unit>
