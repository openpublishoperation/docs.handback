        <trans-unit id="123" translate="yes" xml:space="preserve">
          <source>2.  Click the <bpt id="p1">**</bpt>Compile<ept id="p1">**</ept> tab.</source>
          <target state="translated">2.<bpt id="p1">**</bpt>[コンパイル]<ept id="p1">**</ept> タブをクリックします。</target>       </trans-unit>
