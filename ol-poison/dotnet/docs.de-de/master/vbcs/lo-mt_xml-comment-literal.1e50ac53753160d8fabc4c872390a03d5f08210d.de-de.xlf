        <trans-unit id="122" translate="yes" xml:space="preserve">
          <source>XML comment sections cannot be nested, because <ph id="ph1">`content`</ph> cannot contain the value "--&gt;".</source>
          <target state="translated">XML-Kommentarabschnitte können nicht geschachtelt werden, da <ph id="ph1">`content`</ph> kann nicht den Wert "--&gt;" enthalten.</target>       </trans-unit>
        <trans-unit id="123" translate="yes" xml:space="preserve">
