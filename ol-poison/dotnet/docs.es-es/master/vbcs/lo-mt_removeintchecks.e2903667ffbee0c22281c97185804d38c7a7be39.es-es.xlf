          <target state="translated">/removeintchecks</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Turns overflow-error checking for integer operations on or off.</source>
          <target state="translated">Activa para operaciones con enteros o desactivar la comprobación de errores de desbordamiento.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Syntax</source>
          <target state="translated">Sintaxis</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
